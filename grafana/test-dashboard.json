{
  "__inputs": [
    {
      "name": "DS_INFLUXDB",
      "label": "InfluxDB",
      "description": "InfluxDB data source",
      "type": "datasource",
      "pluginId": "influxdb",
      "pluginName": "InfluxDB"
    }
  ],
  "__requires": [
    {"type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.5.0"},
    {"type": "panel", "id": "timeseries", "name": "Time series", "version": "9.5.0"},
    {"type": "datasource", "id": "influxdb", "name": "InfluxDB", "version": "1.0.0"}
  ],
  "title": "Test Results by Day",
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Passed/Failed per Day",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 9},
      "datasource": {"type": "influxdb", "uid": "${DS_INFLUXDB}"},
      "targets": [
        {
          "query": "SELECT sum(\"passed\") as passed FROM \"test_results\" WHERE $timeFilter GROUP BY time(1d)",
          "refId": "A",
          "resultFormat": "time_series"
        },
        {
          "query": "SELECT sum(\"failed\") as failed FROM \"test_results\" WHERE $timeFilter GROUP BY time(1d)",
          "refId": "B",
          "resultFormat": "time_series"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Total tests per Day",
      "gridPos": {"x": 0, "y": 9, "w": 24, "h": 8},
      "datasource": {"type": "influxdb", "uid": "${DS_INFLUXDB}"},
      "targets": [
        {
          "query": "SELECT sum(\"total\") as total FROM \"test_results\" WHERE $timeFilter GROUP BY time(1d)",
          "refId": "A",
          "resultFormat": "time_series"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "suite",
        "type": "query",
        "datasource": {"type": "influxdb", "uid": "${DS_INFLUXDB}"},
        "query": "SHOW TAG VALUES FROM test_results WITH KEY = \"suite\"",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "hide": 0
      },
      {
        "name": "framework",
        "type": "query",
        "datasource": {"type": "influxdb", "uid": "${DS_INFLUXDB}"},
        "query": "SHOW TAG VALUES FROM test_results WITH KEY = \"framework\"",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "hide": 0
      }
    ]
  }
}
